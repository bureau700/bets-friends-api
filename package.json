{
  "name": "bets-friend-api",
  "version": "0.1.0",
  "description": "GraphQL API for Bets Friends.",
  "main": "dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "env-cmd ts-node --files src/index.ts",
    "start:dist": "env-cmd node dist/src/index.js",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint \"./{src,tests}/**/*.{ts,tsx}\"",
    "test": "jest",
    "precommit": "tsc --skipLibCheck && lint-staged"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/faker": "^4.1.11",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.29.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "fake-tag": "^2.0.0",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.4",
    "sort-package-json": "^1.23.1",
    "sqlite3": "^4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.1",
    "tslib": "^1.11.1",
    "typeorm-fixtures-cli": "^1.5.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "aws-sdk": "^2.656.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.1",
    "express": "^4.17.1",
    "graphql": "^14.1.1",
    "jose": "^1.25.0",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1-alpha.2",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24"
  },
  "engines": {
    "node": "13.8.0"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json",
      "git add"
    ],
    "{src,tests}/**/*.ts?(x)": [
      "eslint --fix",
      "git add"
    ]
  }
}
